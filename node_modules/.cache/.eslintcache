ges":"89","errorCount":0,"fatalErrorCount":0,"warningCount":5,"fixableErrorCount":0,"fixableWarningCount":0,"source":null},{"filePath":"90","messages":"91","suppressedMessages":"92","errorCount":0,"fatalErrorCount":0,"warningCount":1,"fixableErrorCount":0,"fixableWarningCount":0,"source":"93","usedDeprecatedRules":"43"},{"filePath":"94","messages":"95","suppressedMessages":"96","errorCount":0,"fatalErrorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},{"filePath":"97","messages":"98","suppressedMessages":"99","errorCount":0,"fatalErrorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},"/Users/alisonrobichaux/Desktop/Coursework/Capstone/capstone-ecom-final/src/index.js",[],[],[],"/Users/alisonrobichaux/Desktop/Coursework/Capstone/capstone-ecom-final/src/App.js",[],[],"/Users/alisonrobichaux/Desktop/Coursework/Capstone/capstone-ecom-final/src/contexts/ProductContext.js",[],[],"/Users/alisonrobichaux/Desktop/Coursework/Capstone/capstone-ecom-final/src/contexts/SidebarContext.js",[],[],"/Users/alisonrobichaux/Desktop/Coursework/Capstone/capstone-ecom-final/src/pages/ProductDetails.js",[],[],"/Users/alisonrobichaux/Desktop/Coursework/Capstone/capstone-ecom-final/src/pages/Home.js",[],[],"/Users/alisonrobichaux/Desktop/Coursework/Capstone/capstone-ecom-final/src/components/Header.js",[],[],"/Users/alisonrobichaux/Desktop/Coursework/Capstone/capstone-ecom-final/src/components/Footer.js",[],[],"/Users/alisonrobichaux/Desktop/Coursework/Capstone/capstone-ecom-final/src/components/Sidebar.js",[],[],"/Users/alisonrobichaux/Desktop/Coursework/Capstone/capstone-ecom-final/src/components/Product.js",[],[],"/Users/alisonrobichaux/Desktop/Coursework/Capstone/capstone-ecom-final/src/components/CartItem.js",[],[],"/Users/alisonrobichaux/Desktop/Coursework/Capstone/capstone-ecom-final/src/contexts/CartContext.js",["100"],[],"import React, { createContext, useState, useEffect, useContext } from 'react';\n\nexport const CartContext = createContext();\n\nconst CartProvider = ({ children }) => {\n  const [cart, setCart] = useState([]);\n  const [itemAmount, setItemAmount] = useState(0);\n  const [total, setTotal] =  useState(0);\n\n  useEffect(() => {\n    const total = cart.reduce((accumulator, currentItem) => {\n      return accumulator + currentItem.price * currentItem.amount;\n    }, 0);\n    setTotal(total);\n  }, [cart]);\n  \n    // update item amount\n    useEffect(()=> {\n      if (cart) {\n       const amount = cart.reduce((accumulator, currentItem)=> \n       {\n        return accumulator + currentItem.amount;\n       }, 0);\n       setItemAmount(amount);\n      }\n    }, [cart]);\n\n    //add to cart\n  const addToCart = (product, id) => {\n    const newItem = { ...product, amount: 1 };\n\n    const cartItem = cart.find((item) => item.id === id);\n\n    if (cartItem) {\n      const newCart = cart.map((item) =>\n        item.id === id ? { ...item, amount: item.amount + 1 } : item\n      );\n      setCart(newCart);\n    } else {\n      setCart([...cart, newItem]);\n    }\n  };\n\n  const removeFromCart = (id) => {\n    const newCart = cart.filter(item => {\n      return item.id !== id;\n    })\n    setCart(newCart);\n\n  }\n\n  // clear cart\n  const clearCart = ()=> {\n    setCart([]);\n\n  }\n\n//increase amount\nconst increaseAmount = (id) => {\n  const cartItem = cart.find((item) => item.id === id);\n  addToCart(cartItem, id);\n}\n\n// decrease amount\nconst decreaseAmount = (id) => {\n  const cartItem = cart.find((item) => {\n    return item.id === id; \n  });\n  if (cartItem) {\n    const newCart = cart.map((item) => {\n      if (item.id === id) {\n        return {...item, amount: cartItem.amount - 1};\n      } else {\n        return item;\n      }\n    });\n    setCart(newCart);\n  } \n    if (cartItem.amount < 2) {\n      removeFromCart(id);\n    }\n};\n\n  return (\n    <CartContext.Provider \n    value={{ \n    cart, \n    addToCart, \n    removeFromCart, \n    clearCart, \n    increaseAmount,\n    decreaseAmount,\n    itemAmount,\n    total,\n    }}>\n      {children}\n    </CartContext.Provider>\n  );\n};\n\nexport default CartProvider;\n\n","/Users/alisonrobichaux/Desktop/Coursework/Capstone/capstone-ecom-final/src/components/Hero.js",[],[],"/Users/alisonrobichaux/Desktop/Coursework/Capstone/capstone-ecom-final/src/components/Login.js",[],[],"/Users/alisonrobichaux/Desktop/Coursework/Capstone/capstone-ecom-final/src/contexts/AuthContext.js",[],[],"/Users/alisonrobichaux/Desktop/Coursework/Capstone/capstone-ecom-final/src/components/CheckoutPage.js",["101","102","103","104","105"],[],"/Users/alisonrobichaux/Desktop/Coursework/Capstone/capstone-ecom-final/src/components/ViewCartPage.js",["106"],[],"import React, { useContext } from 'react';\nimport { Link } from 'react-router-dom';\nimport { IoMdClose, IoMdAdd, IoMdRemove } from 'react-icons/io';\nimport { CartContext } from '../contexts/CartContext';\n\nconst ViewCartPage = () => {\n    const { cart, addToCart, removeFromCart, increaseAmount, decreaseAmount } = useContext(CartContext);\n\n    const handleAddToCart = (product) => {\n        addToCart(product, product.id);\n    };\n\n    const handleIncreaseAmount = (id) => {\n        increaseAmount(id);\n    };\n\n    const handleDecreaseAmount = (id) => {\n        decreaseAmount(id);\n    };\n\n    // Calculate total price of all items in cart\n    const total = cart.reduce((acc, item) => acc + (item.price * item.amount), 0);\n\n    return (\n        <div className=\"container mx-auto mt-60 flex justify-center\"> {/* Center the content */}\n            <div className=\"grid grid-cols-1 gap-4\">\n                {cart.map((item) => (\n                    <div key={item.id} className=\"bg-white shadow-md flex items-center justify-between p-4 mb-4 rounded-md\">\n                        <img src={item.image} alt={item.name} className=\"w-16 h-16 object-contain mr-4 rounded-md\" />\n                        <div className=\"flex-grow\">\n                            <h3 className=\"text-lg font-semibold\">{item.name}</h3>\n                            <h3 className=\"text-sm\">{item.title}</h3>\n                            <p className=\"text-blue-600 font-semibold\">${item.price}</p>\n                            <div className=\"flex items-center\">\n                                <button onClick={() => handleDecreaseAmount(item.id)} className=\"border border-gray-400 rounded-full p-1 mr-2 hover:bg-gray-200\"><IoMdRemove /></button>\n                                <span>{item.amount}</span>\n                                <button onClick={() => handleIncreaseAmount(item.id)} className=\"border border-gray-400 rounded-full p-1 ml-2 hover:bg-gray-200\"><IoMdAdd /></button>\n                            </div>\n                        </div>\n                        <button onClick={() => removeFromCart(item.id)} className=\"text-red-500 hover:text-red-700\"><IoMdClose /></button>\n                    </div>\n                ))}\n                <div className=\"text-lg font-semibold mb-4\">Total: ${total.toFixed(2)}</div> {/* Display total price */}\n                <div className=\"mt-8 pb-8\"> {/* Add padding to the bottom */}\n                    <Link to=\"/checkout\" className=\"bg-blue-500 text-white px-4 py-2 rounded hover:bg-blue-600\">Proceed to Checkout</Link>\n                </div>\n            </div>\n        </div>\n    );\n};\n\nexport default ViewCartPage;\n","/Users/alisonrobichaux/Desktop/Coursework/Capstone/capstone-ecom-final/src/components/OrderConfirmationPage.js",[],[],"/Users/alisonrobichaux/Desktop/Coursework/Capstone/capstone-ecom-final/src/components/ProductPage.js",[],[],{"ruleId":"107","severity":1,"message":"108","line":1,"column":53,"nodeType":"109","messageId":"110","endLine":1,"endColumn":63},{"ruleId":"107","severity":1,"message":"111","line":5,"column":12,"nodeType":"109","messageId":"110","endLine":5,"endColumn":24},{"ruleId":"107","severity":1,"message":"112","line":5,"colum