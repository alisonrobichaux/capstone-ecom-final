rtProvider = ({ children }) => {\n  const [cart, setCart] = useState([]);\n  const [itemAmount, setItemAmount] = useState(0);\n  const [total, setTotal] =  useState(0);\n\n  useEffect(() => {\n    const total = cart.reduce((accumulator, currentItem) => {\n      return accumulator + currentItem.price * currentItem.amount;\n    }, 0);\n    setTotal(total);\n  }, [cart]);\n  \n    // update item amount\n    useEffect(()=> {\n      if (cart) {\n       const amount = cart.reduce((accumulator, currentItem)=> \n       {\n        return accumulator + currentItem.amount;\n       }, 0);\n       setItemAmount(amount);\n      }\n    }, [cart]);\n\n    //add to cart\n  const addToCart = (product, id) => {\n    const newItem = { ...product, amount: 1 };\n\n    const cartItem = cart.find((item) => item.id === id);\n\n    if (cartItem) {\n      const newCart = cart.map((item) =>\n        item.id === id ? { ...item, amount: item.amount + 1 } : item\n      );\n      setCart(newCart);\n    } else {\n      setCart([...cart, newItem]);\n    }\n  };\n\n  const removeFromCart = (id) => {\n    const newCart = cart.filter(item => {\n      return item.id !== id;\n    })\n    setCart(newCart);\n\n  }\n\n  // clear cart\n  const clearCart = ()=> {\n    setCart([]);\n\n  }\n\n//increase amount\nconst increaseAmount = (id) => {\n  const cartItem = cart.find((item) => item.id === id);\n  addToCart(cartItem, id);\n}\n\n// decrease amount\nconst decreaseAmount = (id) => {\n  const cartItem = cart.find((item) => {\n    return item.id === id; \n  });\n  if (cartItem) {\n    const newCart = cart.map((item) => {\n      if (item.id === id) {\n        return {...item, amount: cartItem.amount - 1};\n      } else {\n        return item;\n      }\n    });\n    setCart(newCart);\n  } \n    if (cartItem.amount < 2) {\n      removeFromCart(id);\n    }\n};\n\n  return (\n    <CartContext.Provider \n    value={{ \n    cart, \n    addToCart, \n    removeFromCart, \n    clearCart, \n    increaseAmount,\n    decreaseAmount,\n    itemAmount,\n    total,\n    }}>\n      {children}\n    </CartContext.Provider>\n  );\n};\n\nexport default CartProvider;\n\n","/Users/alisonrobichaux/Desktop/Coursework/Capstone/capstone-ecom-final/src/components/Hero.js",[],[],"/Users/alisonrobichaux/Desktop/Coursework/Capstone/capstone-ecom-final/src/components/Login.js",[],[],"/Users/alisonrobichaux/Desktop/Coursework/Capstone/capstone-ecom-final/src/contexts/AuthContext.js",[],[],"/Users/alisonrobichaux/Desktop/Coursework/Capstone/capstone-ecom-final/src/components/CheckoutPage.js",["103","104","105","106","107"],[],"import React, { useState } from 'react';\nimport { useNavigate } from 'react-router-dom';\n\nconst CheckoutPage = ({ handleClose }) => {\n    const [shoppingInfo, setShippingInfo] = useState({\n        name: '',\n        address: '',\n        email: '',\n        phone: '',\n    });\n\n    const [paymentInfo, setPaymentInfo] = useState({\n        cardNumber: '',\n        expirationDate: '',\n        cvv: '',\n        billingAddress: '',\n    });\n\n    const navigate = useNavigate(); \n\n    const handleSubmit = (e) => {\n        e.preventDefault();\n        handlePlaceOrder();\n    };\n\n    const handlePlaceOrder = () => {\n      \n\n        navigate('/order-confirmation'); \n    };\n\n    const handleCloseSidebar = () => {\n        handleClose(); \n    };\n\n    return (\n        <div className='container mx-auto h-screen flex flex-col justify-center items-center'>\n            <h2 className='text-4xl font-semibold mb-4'>Checkout</h2>\n            <form onSubmit={handleSubmit} className='bg-white shadow-md px-8 pt-6 pb-8 mb-4 w-full max-w-md'>\n                <div className='mb-4'>\n                    <label htmlFor='name' className='block text-sm font-medium text-gray-700'>Name</label>\n                    <input type='text' id='name' className='mt-1 block w-full border-gray-300 rounded-md shadow-sm\n                    focus:ring-indigo-200 focus:border-indigo-500 sm:text-sm' required />\n                </div>\n                <div className='mb-4'>\n                    <label htmlFor='cardNumber' className='block text-sm font-medium text-gray-700'>Credit Card Number</label>\n                    <input type='text' id='cardNumber' className='mt-1 block w-full border-gray-300 rounded-md shadow-sm\n                    focus:ring-indigo-200 focus:border-indigo-500 sm:text-sm' required />\n                </div>\n                <div className='mb-4'>\n                    <label htmlFor='cvv' className='block text-sm font-medium text-gray-700'>CVV</label>\n                    <input type='text' id='cvv' className='mt-1 block w-full border-gray-300 rounded-md shadow-sm\n                    focus:ring-indigo-200 focus:border-indigo-500 sm:text-sm' required />\n                </div>\n                <button type='submit' className='bg-blue-500 text-white px-4 py-2 rounded-md hover:bg-blue-800\n                 focus:outline-none focus:ring-2 focus:ring-offset-2 focus:ring-blue-500'>Place Order</button>\n            </form>\n        </div>\n    );\n};\n\nexport default CheckoutPage;\n","/Users/alisonrobichaux/Desktop/Coursework/Capstone/capstone-ecom-final/src/components/ViewCartPage.js",["108","109"],[],"import React, { useContext } from 'react';\nimport { Link } from 'react-router-dom';\nimport { IoMdClose, IoMdAdd, IoMdRemove } from 'react-icons/io';\nimport { CartContext } from '../contexts/CartContext';\nimport { SidebarContext } from '../contexts/SidebarContext';\n\nconst ViewCartPage = () => {\n    const { cart, addToCart, removeFromCart, increaseAmount, decreaseAmount } = useContext(CartContext);\n\n    const handleAddToCart = (product) => {\n        addToCart(product, product.id);\n    };\n\n    const handleIncreaseAmount = (id) => {\n        increaseAmount(id);\n    };\n\n    const handleDecreaseAmount = (id) => {\n        decreaseAmount(id);\n    };\n\n\n    const total = cart.reduce((acc, item) => acc + (item.price * item.amount), 0);\n\n    return (\n        <div className=\"container mx-auto mt-60 mb-16 flex justify-center\"> {/* Center the content and add bottom margin */}\n            <div className=\"grid grid-cols-1 gap-4\">\n                {cart.map((item) => (\n                    <div key={item.id} className=\"bg-white shadow-md flex items-center justify-between p-4 mb-4 rounded-md\">\n                        <img src={item.image} alt={item.name} className=\"w-16 h-16 object-contain mr-4 rounded-md\" />\n                        <div className=\"flex-grow\">\n                            <h3 className=\"text-lg font-semibold\">{item.name}</h3>\n                            <h3 className=\"text-sm\">{item.title}</h3>\n                            <p className=\"text-blue-600 font-semibold\">${item.price}</p>\n                            <div className=\"flex items-center\">\n                                <button onClick={() => handleDecreaseAmount(item.id)} className=\"border border-gray-400 rounded-full p-1 mr-2 hover:bg-gray-200\"><IoMdRemove /></button>\n                                <span>{item.amount}</span>\n                                <button onClick={() => handleIncreaseAmount(item.id)} className=\"border border-gray-400 rounded-full p-1 ml-2 hover:bg-gray-200\"><IoMdAdd /></button>\n                            </div>\n                        </div>\n                        <button onClick={() => removeFromCart(item.id)} className=\"text-red-500 hover:text-red-700\"><IoMdClose /></button>\n                    </div>\n                ))}\n                <div className=\"text-lg font-semibold mb-4\">Total: ${total.toFixed(2)}</div> {/* Display total price */}\n                <div className=\"mt-12 pb-12\"> {/* Add padding to the bottom */}\n                    <Link to=\"/checkout\" className=\"bg-blue-500 text-white px-4 py-2 rounded hover:bg-blue-600\">Proceed to Checkout</Link>\n                </div>\n            </div>\n        </div>\n    );\n};\n\nexport default ViewCartPage;\n","/Users/alisonrobichaux/Desktop/Coursework/Capstone/capstone-ecom-final/src/components/OrderConfirmationPage.js",[],[],"/Users/alisonrobichaux/Desktop/Coursework/Capstone/capstone-ecom-final/src/components/ProductPage.js",[],[],{"ruleId":"110","severity":1,"message":"111","line":1,"column":53,"nodeType":"112","messageId":"113","endLine":1,"endColumn":63},{"ruleId":"110","severity":1,"message":"114","line":5,"column":12,"nodeType":"112","messageId":"113","endLine":5,"endColumn":24},{"ruleId":"110","severity":1,"message":"115","line":5,"column":26,"nodeType":"112","messageId":"113","endLine":5,"endColumn":41},{"ruleId":"110","severity":1,"message":"116","line":12,"column":12,"nodeType":"112","messageId":"113","endLine":12,"endColumn":23},{"ruleId":"110","severity":1,"message":"117","line":12,"column":25,"nodeType":"112","messageId":"113","endLine":12,"endColumn":39},{"ruleId":"110","severity":1,"message":"118","line":32,"column":11,"nodeType":"112","messageId":"113","endLine":32,"endColumn":29},{"ruleId":"110","severity":1,"message":"119","line":5,"column":10,"nodeType":"112","messageId":"113","endLine":5,"endColumn":24},{"ruleId":"110","severity":1,"message":"120","line":10,"column":11,"nodeType":"112","messageId":"113","endLine":10,"endColumn":26},"no-unused-vars","'useContext' is defined but never used.","Identifier","unusedVar","'shoppingInfo' is assigned a value but never used.","'setShippingInfo' is assigned a value but never used.","'paymentInfo' is assigned a value but never used.","'setPaymentInfo' is assigned a value but never used.","'handleCloseSidebar' is assigned a value but never used.","'SidebarContext' is defined but never used.","'handleAddToCart' is assigned a value but never used."]